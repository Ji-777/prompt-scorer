{
  "name": "fullstack-chat-repo",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.5.1",
  "description": "Fullstack chat application with LangGraph agents and Next.js UI",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "format": "turbo format",
    "format:check": "turbo format:check",
    "test": "turbo test",
    "test:int": "turbo test:int",
    "clean": "turbo clean && rm -rf node_modules",
    "web:dev": "pnpm --filter web dev",
    "web:build": "pnpm --filter web build",
    "agents:dev": "pnpm --filter agents dev",
    "agents:build": "pnpm --filter agents build",
    "agents:test": "pnpm --filter agents test",
    "agents:test:int": "pnpm --filter agents test:int"
  },
  "devDependencies": {
    "prettier": "^3.5.3",
    "turbo": "^2.3.3",
    "typescript": "~5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-username/fullstack-chat-repo.git"
  },
  "keywords": [
    "langgraph",
    "langchain",
    "nextjs",
    "typescript",
    "chat",
    "ai",
    "agents",
    "monorepo"
  ],
  "license": "MIT"
} 